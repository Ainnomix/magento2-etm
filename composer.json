    "type": "magento2-module",
    "version": "0.0.1",
    "keywords": [
        "magento",
        "magento2",
        "entity-type-manager",
        "etm",
        "staging"
    ],
    "homepage": "https://github.com/Ainnomix/magento2-etm",
    "authors": [
        {
            "name": "Roman Tomchak",
            "email": "romantomchak@gmail.com"
        }
    ],
    "support": {
