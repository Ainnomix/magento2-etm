{
    "name": "ainnomix/module-etm-adminhtml",
    "description": "Magento 2 Entity types manager. Adds full GRUD support in admin area",
    "type": "magento2-module",
    "version": "0.0.1",
    "keywords": [
        "magento",
        "magento2",
        "entity-type-manager",
        "etm",
        "adminhtml"
    ],
    "homepage": "https://github.com/Ainnomix/magento2-etm",
    "authors": [
        {
            "name": "Roman Tomchak",
            "email": "romantomchak@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Ainnomix/magento2-etm/issues"
    },
    "require": {
        "magento/framework": "100.1.*",
        "magento/module-eav": "100.1.*",
        "magento/module-backend": "100.1.*",
        "magento/module-ui": "100.1.*",
        "ainnomix/etm-core": "~0.1"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Ainnomix\\EtmAdminhtml\\": ""
        }
    },
    "suggest": {
        "ainnomix/module-etm-api": "Provides API functionality for custom entity types",
        "ainnomix/module-etm-staging": "Adds staging functionality for custom entity types. Allowed only for Magento Enterprise Edition"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
